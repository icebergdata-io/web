{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/car-rental",
      "destination": "/car-rental.html"
    },
    {
      "source": "/press",
      "destination": "/press.html"
    },
    {
      "source": "/services",
      "destination": "/services.html"
    },
    {
      "source": "/case-studies",
      "destination": "/case-studies.html"
    },
    {
      "source": "/careers",
      "destination": "/careers.html"
    },
    {
      "source": "/private-case-studies",
      "destination": "/index.html"
    },
    {
      "source": "/case-study/:sector/:slug",
      "destination": "/case-study/:sector/:slug.html"
    },
    {
      "source": "/careers/:jobId",
      "destination": "/careers/:jobId.html"
    },
    {
      "source": "/private-case-study/:caseId/:accessToken",
      "destination": "/index.html"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'self' 'unsafe-inline' 'unsafe-eval' https://www.googletagmanager.com https://www.google-analytics.com; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com; font-src 'self' https://fonts.gstatic.com; img-src 'self' data: https: blob:; connect-src 'self' https://www.google-analytics.com https://www.googletagmanager.com; frame-src 'self' https://calendly.com; object-src 'none'; base-uri 'self'; form-action 'self'; frame-ancestors 'none'; upgrade-insecure-requests;"
        }
      ]
    },
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate, proxy-revalidate"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, s-maxage=3600, stale-while-revalidate=86400"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/refund",
      "destination": "/refund-policy",
      "permanent": true
    },
    {
      "source": "/dm",
      "destination": "https://calendly.com/icedata/dm",
      "permanent": true
    },
    {
      "source": "/interview",
      "destination": "https://calendar.app.google/UpNA2c7WWbQiBH4AA",
      "permanent": true
    },
    {
      "source": "/test",
      "destination": "https://calendar.app.google/QBsvxCi4z24MKkFX6",
      "permanent": true
    },
    {
      "source": "/case-study/:sector/:slug.html",
      "destination": "/case-study/:sector/:slug",
      "permanent": true
    },
    {
      "source": "/careers/:jobId.html",
      "destination": "/careers/:jobId",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "destination": "https://www.icebergdata.co/$1",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "icebergdata.co"
        }
      ]
    }
  ],
  "functions": {
    "api/contact/index.js": {
      "maxDuration": 10
    },
    "api/applications/index.js": {
      "maxDuration": 10
    }
  },
  "build": {
    "env": {}
  },
  "framework": "vite",
  "buildCommand": "node -v && npm run build:seo",
  "devCommand": "npm run dev",
  "installCommand": "npm install --legacy-peer-deps"
}