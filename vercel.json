{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/dm",
      "destination": "https://calendly.com/icedata/dm",
      "permanent": true
    }
  ],
  "functions": {
    "api/contact/index.js": {
      "runtime": "@vercel/node@2.15.10",
      "maxDuration": 10
    }
  },
  "build": {
    "env": {
      "NODE_VERSION": "18.x"
    }
  }
} 